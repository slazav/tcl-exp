#!/usr/bin/gnuplot


w0=1000
dw=10

A=1.1
B=0.1
C=1.2
D=2.2
w0=1023
dw=11

X(w) = A - (C*dw - D*(w-w0))/((w-w0)**2 + dw**2)
Y(w) = B - (C*(w-w0) - D*dw)/((w-w0)**2 + dw**2)

set nokey

#set parametric
plot \
  "fit_res.dat" using 1:2 with points lc 2,\
  "fit_res.dat" using 1:3 with points lc 2,\
  "fit_res.dat" using 1:4 with lines lc 7,\
  "fit_res.dat" using 1:5 with lines lc 7,\
  X(x) lc 0, Y(x) lc 0

pause -1
